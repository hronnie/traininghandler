<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <class name="com.codeproj.traininghandler.model.Address" table="Address" catalog="traininghandler">
        <id access="field" column="addressId" name="addressId" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <property name="postalCode" type="string">
            <column name="postalCode" length="15" not-null="true" />
        </property>
        <property name="city" type="string">
            <column name="city" length="50" not-null="false" />
        </property>
        <property name="street" type="string">
            <column name="street" length="70" not-null="false" />
        </property>
        <property name="houseNo" type="string">
            <column name="houseNo" length="10" not-null="false" />
        </property>
        <property name="country" type="string">
            <column name="country" length="50" not-null="false" />
        </property>
        <property name="oneLineAddress" type="string">
            <column name="oneLineAddress" length="100" not-null="false" />
        </property>
        <property name="isTrainingPlace" type="boolean">
            <column name="isTrainingPlace" not-null="true" />
        </property>
        <property name="isAppointmentPlace" type="boolean">
            <column name="isAppointmentPlace" not-null="true" />
        </property>
        <set name="trainingTypes" table="TrainingType" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="addressId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.TrainingType" />
        </set>
        <set name="users" table="User" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="addressId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.User" />
        </set>
        <set name="trainings" table="Training" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="addressId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.Training" />
        </set>
        <set name="appointments" table="Appointment" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="addressId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.Appointment" />
        </set>
    </class>
 
     <class name="com.codeproj.traininghandler.model.Appointment" table="Appointment" catalog="traininghandler">
        <id access="field" column="appointmentId" name="appointmentId" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <many-to-one name="userByHealerId" class="com.codeproj.traininghandler.model.User" fetch="select">
            <column name="healerId" not-null="true" />
        </many-to-one>
        <many-to-one name="userByPatientId" class="com.codeproj.traininghandler.model.User" fetch="select">
            <column name="patientId" not-null="true" />
        </many-to-one>
        <many-to-one name="address" class="com.codeproj.traininghandler.model.Address" fetch="select">
            <column name="addressId" not-null="true" />
        </many-to-one>
        <property name="fromDateTime" type="timestamp">
            <column name="fromDateTime" length="19" not-null="true" />
        </property>
        <property name="toDateTime" type="timestamp">
            <column name="toDateTime" length="19" not-null="true" />
        </property>
        <property name="description" type="string">
            <column name="description" length="300" />
        </property>
        <property name="isEmailConfirmation" type="boolean">
            <column name="isEmailConfirmation" not-null="true" />
        </property>
    </class>
 
     <class name="com.codeproj.traininghandler.model.CompletedUserTraining" table="CompletedUserTraining" catalog="traininghandler">
        <id access="field" column="completedUserTrainingId" name="completedUserTrainingId" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <many-to-one name="user" class="com.codeproj.traininghandler.model.User" fetch="select">
            <column name="userId" not-null="true" />
        </many-to-one>
        <many-to-one name="trainingType" class="com.codeproj.traininghandler.model.TrainingType" fetch="select">
            <column name="trainingTypeId" not-null="true" />
        </many-to-one>
        <property name="completedDate" type="timestamp">
            <column name="completedDate" length="19" not-null="true" />
        </property>
    </class>
 
     <class name="com.codeproj.traininghandler.model.UserRole" table="UserRole" catalog="traininghandler">
        <id access="field" column="userRoleId" name="userRoleId" unsaved-value="0">
            <generator class="increment"/>
        </id>        
        <many-to-one name="user" class="com.codeproj.traininghandler.model.User" fetch="select">
            <column name="userId" not-null="true" />
        </many-to-one>
        <many-to-one name="role" class="com.codeproj.traininghandler.model.Role" fetch="select">
            <column name="roleId" not-null="true" />
        </many-to-one>
    </class>
 
     <class name="com.codeproj.traininghandler.model.Role" table="Role" catalog="traininghandler">
        <id access="field" column="roleId" name="roleId" unsaved-value="0">
            <generator class="increment"/>
        </id>    
        <property name="roleName" type="string">
            <column name="roleName" length="50" />
        </property>
    </class>
 
 
    <class name="com.codeproj.traininghandler.model.EmailTemplate" table="EmailTemplate" catalog="traininghandler">
        <id access="field" column="emailTemplateId" name="emailTemplateId" unsaved-value="0">
            <generator class="increment"/>
        </id>          
        <property name="subject" type="string">
            <column name="subject" not-null="true" />
        </property>
        <property name="body" type="string">
            <column name="body" length="65535" not-null="true" />
        </property>
        <set name="sentTrainingEmails" table="SentTrainingEmail" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="emailTemplateId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.SentTrainingEmail" />
        </set>
    </class>


    <class name="com.codeproj.traininghandler.model.GeneralSettings" table="GeneralSettings" catalog="traininghandler">
        <id access="field" column="generalSettingsId" name="generalSettingsId" unsaved-value="0">
            <generator class="increment"/>
        </id>            
        <property name="name" type="string">
            <column name="name" not-null="true" />
        </property>
        <property name="value" type="string">
            <column name="value" not-null="true" />
        </property>
    </class>


    <class name="com.codeproj.traininghandler.model.Log" table="Log" catalog="traininghandler">
        <id access="field" column="logId" name="logId" unsaved-value="0">
            <generator class="increment"/>
        </id>          
        <many-to-one name="user" class="com.codeproj.traininghandler.model.User" fetch="select">
            <column name="who" not-null="true" />
        </many-to-one>
        <property name="action" type="string">
            <column name="action" length="50" not-null="true" />
        </property>
        <property name="message" type="string">
            <column name="message" />
        </property>
        <property name="dateTime" type="timestamp">
            <column name="dateTime" length="19" not-null="true" />
        </property>
    </class>


    <class name="com.codeproj.traininghandler.model.SentTrainingEmail" table="SentTrainingEmail" catalog="traininghandler">
        <id access="field" column="sentTrainingEmailId" name="sentTrainingEmailId" unsaved-value="0">
            <generator class="increment"/>
        </id>             
        <many-to-one name="training" class="com.codeproj.traininghandler.model.Training" fetch="select">
            <column name="trainingId" not-null="true" />
        </many-to-one>
        <many-to-one name="user" class="com.codeproj.traininghandler.model.User" fetch="select">
            <column name="userId" not-null="true" />
        </many-to-one>
        <many-to-one name="emailTemplate" class="com.codeproj.traininghandler.model.EmailTemplate" fetch="select">
            <column name="emailTemplateId" not-null="true" />
        </many-to-one>
        <property name="traineeEmailToken" type="string">
            <column name="traineeEmailToken" length="30" not-null="true" />
        </property>
        <property name="isConfirmed" type="boolean">
            <column name="isConfirmed" not-null="true" />
        </property>
        <property name="isAttend" type="boolean">
            <column name="isAttend" not-null="true" />
        </property>
    </class>


    <class name="com.codeproj.traininghandler.model.Training" table="Training" catalog="traininghandler">
        <id access="field" column="trainingId" name="trainingId" unsaved-value="0">
            <generator class="increment"/>
        </id>     
        <many-to-one name="trainingType" class="com.codeproj.traininghandler.model.TrainingType" fetch="select">
            <column name="trainingTypeId" not-null="true" />
        </many-to-one>
        <many-to-one name="address" class="com.codeproj.traininghandler.model.Address" fetch="select">
            <column name="addressId" not-null="true" />
        </many-to-one>
        <property name="nameOfMaster" type="string">
            <column name="nameOfMaster" length="100" not-null="true" />
        </property>
        <property name="dateTime" type="timestamp">
            <column name="dateTime" length="19" not-null="true" />
        </property>
        <property name="description" type="string">
            <column name="description" length="200" />
        </property>
        <property name="autoInvite" type="boolean">
            <column name="autoInvite" not-null="true" />
        </property>
        <set name="sentTrainingEmails" table="SentTrainingEmail" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="trainingId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.SentTrainingEmail" />
        </set>
        <set name="users" table="User" 
        	inverse="false" lazy="true" fetch="select" cascade="all" >
            <key>
                <column name="trainingId" not-null="true" />
            </key>
            <many-to-many entity-name="com.codeproj.traininghandler.model.User">
                <column name="userId" not-null="true" />
            </many-to-many>
        </set>	
    </class>


    <class name="com.codeproj.traininghandler.model.TrainingPrerequisite" table="TrainingPrerequisite" catalog="traininghandler">
        <id access="field" column="trainingPrerequisiteId" name="trainingPrerequisiteId" unsaved-value="0">
            <generator class="increment"/>
        </id>           
        <many-to-one name="dependentTrainingType" class="com.codeproj.traininghandler.model.TrainingType" fetch="select">
            <column name="dependentTrainingTypeId" not-null="true" />
        </many-to-one>
        <many-to-one name="prerequisiteTrainingType" class="com.codeproj.traininghandler.model.TrainingType" fetch="select">
            <column name="prerequisiteTrainingTypeId" not-null="true" />
        </many-to-one>
        <property name="betweenMonth" type="short">
            <column name="betweenMonth" not-null="true" />
        </property>
    </class>


    <class name="com.codeproj.traininghandler.model.TrainingType" table="TrainingType" catalog="traininghandler">
        <id access="field" column="trainingTypeId" name="trainingTypeId" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <property access="field" column="name" name="name"/>
        <property access="field" column="levelNo" name="levelNo"/>
        <property access="field" column="description" name="description"/>
    </class>


    <class name="com.codeproj.traininghandler.model.User" table="User" catalog="traininghandler">
        <id access="field" column="userId" name="userId" unsaved-value="0">
            <generator class="increment"/>
        </id>             
        <many-to-one name="address" class="com.codeproj.traininghandler.model.Address" fetch="select">
            <column name="addressId" not-null="true" />
        </many-to-one>
        <many-to-one name="userType" class="com.codeproj.traininghandler.model.UserType" fetch="select">
            <column name="userTypeId" not-null="true" />
        </many-to-one>
        <property name="displayName" type="string">
            <column name="displayName" length="50" />
        </property>
        <property name="password" type="string">
            <column name="password" />
        </property>
        <property name="name" type="string">
            <column name="name" length="100" not-null="true" />
        </property>
        <property name="userName" type="string">
            <column name="userName" length="30" />
        </property>
        <property name="dob" type="timestamp">
            <column name="dob" length="19" />
        </property>
        <property name="email" type="string">
            <column name="email" length="80" not-null="true" unique="true" />
        </property>
        <property name="mobileNo" type="string">
            <column name="mobileNo" length="50" />
        </property>
        <property name="isEnabled" type="boolean">
            <column name="isEnabled" not-null="true" />
        </property>
        <property name="created" type="timestamp">
            <column name="created" length="19" />
        </property>
        <property access="field" column="locale" name="locale"/>
        <set name="userLevels" table="UserLevel" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="userId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.UserLevel" />
        </set>
        <set name="appointmentsForPatientId" table="Appointment" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="patientId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.Appointment" />
        </set>
        <set name="appointmentsForHealerId" table="Appointment" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="healerId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.Appointment" />
        </set>
        <set name="sentTrainingEmails" table="SentTrainingEmail" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="userId" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.SentTrainingEmail" />
        </set>
        <set name="logs" table="Log" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="who" not-null="true" />
            </key>
            <one-to-many class="com.codeproj.traininghandler.model.Log" />
        </set>
		<set name="trainings" table="Training" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="userId" not-null="true" />
            </key>
            <many-to-many entity-name="com.codeproj.traininghandler.model.Training">
                <column name="trainingId" not-null="true" />
            </many-to-many>
        </set>
    </class>

    <class name="com.codeproj.traininghandler.model.UserLevel" table="UserLevel" catalog="traininghandler">
        <id access="field" column="userLevelId" name="userLevelId" unsaved-value="0">
            <generator class="increment"/>
        </id>          
        <many-to-one name="trainingType" class="com.codeproj.traininghandler.model.TrainingType" fetch="select">
            <column name="trainingTypeId" not-null="true" />
        </many-to-one>
        <many-to-one name="user" class="com.codeproj.traininghandler.model.User" fetch="select">
            <column name="userId" not-null="true" />
        </many-to-one>
        <property name="completedDate" type="timestamp">
            <column name="completedDate" length="19" />
        </property>
    </class>

    <class name="com.codeproj.traininghandler.model.UserType" table="UserType" catalog="traininghandler">
        <id access="field" column="userTypeId" name="userTypeId" unsaved-value="0">
            <generator class="increment"/>
        </id>     
        <property name="userTypeName" type="string">
            <column name="userTypeName" length="50" />
        </property>
    </class>

 </hibernate-mapping>
